{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime",
    "standard-jsx"
  ],
  "plugins": ["react"],
  "rules": {
    //"semi": ["error", "always"],
    //"quotes": ["error", "single"],
     "semi": ["error", "never"],
     "quotes": ["error", "never"],
    "react/react-in-jsx-scope": "off",
    "@typescript-eslint/no-unused-vars": ["warn"]
  }
}
